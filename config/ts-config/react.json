{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"incremental": true,
		"lib": [
			"es2023",
			"dom",
			"dom.iterable"
		],
		"jsx": "react-jsx"
	},
	"include": [
		"${configDir}/src/**/*"
	],
	"exclude": [
		"${configDir}/node_modules",
		"${configDir}/docs"
	]
}