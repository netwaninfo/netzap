{
  "$schema": "https://swc.rs/schema.json",
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    }
  },
  "exclude": [".*\\.spec.ts", ".*\\.d.ts"]
}
